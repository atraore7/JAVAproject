<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="sample.CustomerAppointmentsController"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <Label layoutX="162.0" layoutY="36.0" text="Customers' Appointments">
            <font>
                <Font name="System Bold" size="24.0" />
            </font>
        </Label>
        <ComboBox fx:id="customerCombo" layoutX="190.0" layoutY="95.0" prefHeight="25.0" prefWidth="165.0" promptText="Select Customer" />
        <Button onAction="#handleSubmitButton" layoutX="375.0" layoutY="95.0" mnemonicParsing="false" text="Submit" />
        <Label fx:id="label" layoutX="111.0" layoutY="183.0" prefHeight="35.0" prefWidth="416.0">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
        <Button onAction="#handleBackButton" layoutX="483.0" layoutY="345.0" mnemonicParsing="false" text="Back" />
    </children>


</AnchorPane>
